{
    "background": {
        "page": "background.html"
    },
    "content_scripts": [ {
        "all_frames": true,
        "js": [ "js/jquery-2.1.0.min.js", "js/content_script.js" ],
        "matches": [ "http://*/*", "https://*/*" ]
    } ],
    "icons": {
        "16": "img/icon_16.png",
        "256": "img/icon_256.png"
    },
    "name": "iShark",
    "manifest_version": 2,
    "minimum_chrome_version": "20",
    "permissions": [ "tabs", "contextMenus", "http://ishark.dev/"],
    "version": "0.0.1"
}
